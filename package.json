    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.2.0",
    "axios": "^0.20.0",
    "bignumber.js": "^9.0.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "echarts": "^4.9.0",
    "uuid": "^8.3.0",
    "vant": "^2.10.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.21.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vxe-table": "^2.9.24",
    "xe-utils": "^2.7.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-useless-escape": 0,
      "quotes": [
        0,
        "single"
      ],
      "max-len": [
        "error",
        {
          "code": 300
        }
      ],
      "comma-dangle": 0,
      "operator-linebreak": 0,
      "semi": 0,
      "eol-last": 0,
      "no-extend-native": 0,
      "no-eval": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
